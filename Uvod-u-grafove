#include <stdio.h>




int main()
{

    int n; // Broj cvorova
    int m; // Broj grana
    int x,y,d;
    
    scanf("%d%d", &n, &m);
    
    int a[n][n]; 
    
    for(int i=0;i<n;i++)
        for(int j=0; j<n; j++)
            a[i][j]=0;
    
    for(int i=0; i<m; i++){
        
        scanf("%d%d%d",&x,&y,&d);
        a[x][y]=d;
        a[y][x]=d;
        
    }
    

    
    for(int i=0;i<n;i++)
    {
        for(int j=0; j<n; j++)
        {
            printf("%d\t",a[i][j]);
            
        }
    
     printf("\n");
    }    
        
    
    
}
